{
  "name": "decomplex-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "build:ts": "tsc -p tsconfig.json",
    "build:assets": "node scripts/copyDbAssets.mjs",
    "build": "npm run build:ts && npm run build:assets",
    "start": "node dist/server.js",
    "start:prod": "node dist/database/runMigrations.js && node dist/server.js",
    "start:prod:seed": "node dist/database/runMigrations.js && node dist/database/runSeeders.js && node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:fresh": "sequelize db:migrate:undo:all && npm run db:migrate",
    "db:fresh:seed": "npm run db:fresh && npm run db:seed",
    "db:migration:create": "sequelize migration:create --name",
    "db:seed:create": "sequelize seed:create --name",
    "with:test": "node scripts/withEnv.mjs --env-file .env.test --node-env test --",
    "with:e2e": "node scripts/withEnv.mjs --env-file .env.e2e --node-env test --",
    "with:e2e:docker": "node scripts/withEnv.mjs --env-file .env.e2e.docker --node-env test --",
    "test:e2e:docker": "npm run with:e2e:docker -- jest --config e2e/jest.config.cjs",
    "test": "npm run with:test -- jest",
    "test:watch": "npm run with:test -- jest --watch",
    "test:coverage": "npm run with:test -- jest --coverage",
    "db:test:reset": "npm run with:test -- sequelize db:migrate:undo:all --env test && npm run with:test -- sequelize db:migrate --env test",
    "db:test:reset:seed": "npm run db:test:reset && npm run with:test -- sequelize db:seed:all --env test",
    "db:e2e:reset": "npm run with:e2e -- sequelize db:migrate:undo:all --env test && npm run with:e2e -- sequelize db:migrate --env test",
    "db:e2e:reset:seed": "npm run db:e2e:reset && npm run with:e2e -- sequelize db:seed:all --env test",
    "test:e2e": "npm run with:e2e -- jest --config e2e/jest.config.cjs",
    "env:test:print": "npm run with:test -- node -e \"console.log('NODE_ENV=',process.env.NODE_ENV); console.log('ENV_LOADED=',!!process.env.DATABASE_URL || !!process.env.DB_HOST)\"",
    "env:e2e:print": "npm run with:e2e -- node -e \"console.log('NODE_ENV=',process.env.NODE_ENV); console.log('ENV_LOADED=',!!process.env.DATABASE_URL || !!process.env.DB_HOST)\"",
    "verify": "npm run check && npm test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit && tsc -p tsconfig.test.json --noEmit && tsc -p e2e/tsconfig.json --noEmit",
    "format": "prettier --write .",
    "check": "npm run lint && npm run typecheck",
    "fix": "npm run lint:fix && npm run format"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tyomay1999/decomplex-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tyomay1999/decomplex-backend/issues"
  },
  "homepage": "https://github.com/Tyomay1999/decomplex-backend#readme",
  "dependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.6",
    "@types/express-fileupload": "^1.5.1",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.3",
    "@types/nodemailer": "^7.0.4",
    "@types/on-finished": "^2.3.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "amqplib": "^0.10.9",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-fileupload": "^1.5.2",
    "helmet": "^8.1.0",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.3",
    "nodemailer": "^7.0.11",
    "on-finished": "^2.4.1",
    "openapi-types": "^12.1.3",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "redis": "^5.10.0",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typescript": "^5.9.3",
    "umzug": "^3.8.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript-eslint": "^8.49.0"
  }
}
